	<Script file="ItemObjects.lua"></Script>

	<Script file="Babylonian.lua"></Script>
	<Script file="ItemStrings.lua"></Script>
	<Script file="ItemLocale.lua"></Script>

	<Script file="Itemizer.lua"></Script>
	<Script file="ItemCore.lua"></Script>
	<Script file="ItemUtil.lua"></Script>
	<Script file="ItemStorage.lua"></Script>
	<Script file="ItemScanner.lua"></Script>

	<!-- This is our main frame -->
	<Frame name="ItemizerFrame" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				Itemizer.OnLoad();
			</OnLoad>
			<OnEvent>
				Itemizer.Core.OnEvent(event, arg1);
			</OnEvent>
		</Scripts>
	</Frame>

	<!-- These are our two tooltips, the first is the one we use to show tooltips on our searchframe, and the second one is the one we use to gather data.  -->
	<GameTooltip name="ItemizerTooltip" inherits="GameTooltipTemplate" parent="UIParent" hidden="true"/>
	<GameTooltip name="ItemizerHidden" inherits="GameTooltipTemplate" parent="UIParent" hidden="true"/>

	<!-- This frame is used to incrementally parse the items in the scan stack without causing a massive hang when many items are added to the stack (Like when the ItemCache is scanned) -->
	<Frame name="ItemizerScanFrame" parent="UIParent">
		<Scripts>
			<OnUpdate>
				Itemizer.Scanner.OnUpdate(arg1);
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui>