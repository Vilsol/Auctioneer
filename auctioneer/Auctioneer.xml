	<Frame name="Auctioneer" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				Auctioneer_OnLoad();
				this:RegisterEvent("ADDON_LOADED")
			</OnLoad>
			<OnEvent>
				if (event == "ADDON_LOADED" and arg1 == "Auctioneer") then
					Auctioneer_AddonLoaded()
					this:UnregisterEvent("ADDON_LOADED")
				end
			</OnEvent>
		</Scripts>
		<Frames>
			<Button name="BrowseScanButton" inherits="UIPanelButtonTemplate" hidden="true">
				<Size>
					<AbsDimension x="80" y="22"/>
				</Size>
				<Scripts>
					<OnClick>
						Auctioneer_RequestAuctionScan();
					</OnClick>
					<OnUpdate>
						if (Auctioneer_CanScan()) then
							this:Enable();
						else
							this:Disable();
						end
					</OnUpdate>
				</Scripts>
			</Button>
		</Frames>
	</Frame>
